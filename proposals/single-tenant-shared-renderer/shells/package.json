		"@types/react": "18.0.15",
		"@types/react-dom": "18.0.6",
		"next": "12.2.4",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"rollup": "2.77.2",
		"rollup-plugin-filesize": "9.1.2",
		"rollup-plugin-smart-asset": "2.1.2",
		"typescript": "4.7.4"
	},
	"peerDependencies": {
		"@framework/design-system": "workspace:*",
		"next": ">=12.0.0",
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"eslintConfig": {
