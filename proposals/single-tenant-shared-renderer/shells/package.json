{
	"name": "@single-tenant-shared-renderer/shells",
	"private": true,
	"exports": {
		"./brand-blue": {
			"import": {
				"types": "./dist/brand-blue/index.d.ts",
				"source": "./src/brand-blue/index.tsx",
				"default": "./dist/brand-blue.js"
			},
			"require": {
				"types": "./dist/brand-blue/index.d.ts",
				"source": "./src/brand-blue/index.tsx",
				"default": "./dist/brand-blue.js"
			}
		},
		"./brand-red": {
			"import": {
				"types": "./dist/brand-red/index.d.ts",
				"source": "./src/brand-red/index.tsx",
				"default": "./dist/brand-red.js"
			},
			"require": {
				"types": "./dist/brand-red/index.d.ts",
				"source": "./src/brand-red/index.tsx",
				"default": "./dist/brand-red.js"
			}
		}
	},
	"typesVersions": {
		"*": {
			"brand-blue": [
