{
	"name": "monorepo-poc-white-label",
	"description": "White-label solution proposals",
	"private": true,
	"version": "0.0.0",
	"author": {
		"name": "Ayoub Adib",
		"email": "adbayb@gmail.com",
		"url": "https://twitter.com/adbayb"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/adbayb/poc-white-label.git"
	},
	"bugs": {
		"url": "https://github.com/adbayb/poc-white-label/issues"
	},
	"homepage": "https://github.com/adbayb/poc-white-label#readme",
	"license": "MIT",
	"scripts": {
		"postinstall": "scripts setup",
		"clean": "turbo run clean",
		"verify": "turbo run verify",
		"fix": "turbo run fix",
		"test": "turbo run test",
		"build": "turbo run build",
		"start:single-tenant-shared-renderer:brand-red": "BRAND_ID=brand-red turbo run start --filter=@single-tenant-shared-renderer/* --filter=@framework/*",
		"start:single-tenant-shared-renderer:brand-blue": "BRAND_ID=brand-blue turbo run start --filter=@single-tenant-shared-renderer/* --filter=@framework/*",
		"start:single-tenant-separate-renderer": "turbo run start --filter=@single-tenant-separate-renderer/* --filter=@framework/*",
		"start:multi-tenant": "turbo run start --filter=@multi-tenant/* --filter=@framework/*"
	},
	"devDependencies": {
		"@adbayb/scripts": "0.14.0",
		"turbo": "1.4.3",
		"typescript": "4.7.4"
	},
	"prettier": "@adbayb/prettier-config",
	"eslintConfig": {
		"extends": "@adbayb",
		"root": true,
		"rules": {
			"import/no-default-export": "off"
		}
	}
}